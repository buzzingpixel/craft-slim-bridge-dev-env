{
    "name": "buzzingpixel/craft-slim-bridge-dev-env",
    "description": "Dev environment for Craft Slim Bridge",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "TJ Draper",
            "email": "tj@buzzingpixel.com",
            "homepage": "https://buzzingpixel.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "/craft-slim-bridge"
        }
    ],
    "require": {
        "php": "^8.0",
        "craftcms/cms": "^3.7"
    },
    "require-dev": {
        "buzzingpixel/twig-dumper": "^1.0",
        "filp/whoops": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "config": {
        "bin-compat": "full",
        "sort-packages": true
    }
}
